
def arrayEvenAndOdd(arr, n):
	
	ind = 0;
	a = [0 for i in range(n)]
	
	for i in range(n):
		if (arr[i] % 2 == 0):
			a[ind] = arr[i]
			ind += 1

	for i in range(n):
		if (arr[i] % 2 != 0):
			a[ind] = arr[i]
			ind += 1

	for i in range(n):
		print(a[i], end = " ")
		
	print()

# Driver code
arr = [ 1, 3, 2, 4, 7, 6, 9, 10 ]
n = len(arr)

# Function call
arrayEvenAndOdd(arr, n)

